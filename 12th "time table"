// program which print to screen time table

#include <stdio.h>
//'a' - the digit of a users

int a, i, j;

int main()
{
    printf("set a digit of time table   ");
    scanf("%d", &a);

    for (i = 1; i <= a; i++)
    {
        printf("\n\r    %d", i);

        for (j = 2; j <= a; j++)
        {
            printf("   %d", i * j);
        }
    }

    printf("\n finishing, press any key");
    getch();

    return 0;
}
